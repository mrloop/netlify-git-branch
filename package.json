{
  "name": "netlify-git-branch-deploy",
  "version": "1.0.2",
  "private": false,
  "description": "Feature branch staging environments with netlfiy and git",
  "homepage": "https://github.com/mrloop/netlify-git-branch-deploy",
  "repository": "https://github.com/mrloop/netlify-git-branch-deploy",
  "license": "MIT",
  "author": "mrloop <ewan@mrloop.com>",
  "type": "module",
  "main": "src/index.js",
  "bin": {
    "netlify-git-branch-deploy": "./bin/run.js",
    "ngbd": "./bin/run.js"
  },
  "scripts": {
    "lint": "eslint .",
    "release": "release-it",
    "test": "qunit tests/test.js"
  },
  "dependencies": {
    "execa": "^6.0.0",
    "netlify": "8.0.4",
    "netlify-cli": "7.0.5",
    "puppeteer": "11.0.0"
  },
  "devDependencies": {
    "eslint": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "prettier": "2.4.1",
    "qunit": "2.17.2",
    "release-it": "14.11.8",
    "release-it-lerna-changelog": "4.0.1"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "release-it": {
    "plugins": {
      "release-it-lerna-changelog": {
        "infile": "CHANGELOG.md",
        "launchEditor": true
      }
    },
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": true,
      "tokenRef": "GITHUB_AUTH"
    }
  },
  "volta": {
    "node": "16.13.0",
    "yarn": "1.22.17"
  }
}
