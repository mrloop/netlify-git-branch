{
  "name": "netlify-git-branch",
  "description": "Feature branch staging environments with netlify and git.",
  "version": "2.0.0",
  "author": "mrloop <ewan@mrloop.com>",
  "bin": {
    "netlify-git-branch": "./bin/run"
  },
  "bugs": "https://github.com/mrloop/netlify-git-branch/issues",
  "dependencies": {
    "@oclif/command": "^1.8.16",
    "@oclif/core": "1.9",
    "@oclif/plugin-help": "5",
    "execa": "5.1.1",
    "netlify": "9.0.0",
    "netlify-cli": "10.10.0",
    "puppeteer": "13.7.0"
  },
  "devDependencies": {
    "@oclif/test": "2.1.0",
    "@types/chai": "4.3.1",
    "@types/execa": "2.0.0",
    "@types/mocha": "9.1.1",
    "@types/node": "16.11.45",
    "chai": "4.3.6",
    "eslint": "7.32.0",
    "eslint-config-oclif": "4.0.0",
    "eslint-config-oclif-typescript": "1.0.2",
    "eslint-plugin-json": "3.1.0",
    "globby": "11.1.0",
    "mocha": "10.0.0",
    "oclif": "3.1.1",
    "release-it": "15.1.2",
    "release-it-lerna-changelog": "4.0.1",
    "ts-node": "10.9.1",
    "tslib": "2.4.0",
    "typescript": "4.7.4"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "release-it": {
    "plugins": {
      "release-it-lerna-changelog": {
        "infile": "CHANGELOG.md",
        "launchEditor": true
      }
    },
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": true,
      "tokenRef": "GITHUB_AUTH"
    }
  },
  "volta": {
    "node": "16.16.0",
    "yarn": "1.22.19"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/mrloop/netlify-git-branch",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "netlify-git-branch",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "mrloop/netlify-git-branch",
  "scripts": {
    "build": "rm -rf lib && tsc -b",
    "lint": "eslint .",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "release": "release-it",
    "test": "DEBUG=netlify-git-branch:* mocha --forbid-only \"test/**/*.test.ts\" --no-timeout",
    "version": "oclif readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
